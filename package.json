{
  "name": "interval-timer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:deploy": "NEXT_DISABLE_ESLINT=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:strict": "next lint --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:dev": "eslint . --ext .js,.jsx,.ts,.tsx --config .eslintrc.dev.json",
    "typecheck": "tsc --noEmit",
    "prebuild": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 && tsc --noEmit",
    "postinstall": "npm install --save-dev @typescript-eslint/eslint-plugin@^7.0.0 @typescript-eslint/parser@^7.0.0"
  },
  "dependencies": {
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.6.2",
    "lucide-react": "^0.424.0",
    "next": "^15.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
